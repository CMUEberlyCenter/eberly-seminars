<% if params[:status] == 'published' %>
 <%= render 'published_seminars' %>
 <%= render 'expired_seminars' %>

<% elsif params[:status] == 'drafts' %>
 <%= render 'development_seminars' %>

<% else %>
  <%= render 'published_seminars' %>
  <%= render 'development_seminars' %>
  <%= render 'expired_seminars' %>

<% end %>
